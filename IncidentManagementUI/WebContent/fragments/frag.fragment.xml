<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<Dialog title="Incident Details" stretch="true">
		<VBox class="inctureCapexBox">
			<Panel expandable="false" expanded="false" headerText="USER INFORMATION"
				width="auto" class="tabClass">
				<l:Grid defaultSpan="L6 M12 S12">
					<HBox>
						<Label text="User Id:" tooltip="Project Coordinator" width="100%"
							class="inctureUserProfileLblClass1" />
						<Input value="{ApprovalModel>/user/userId}" editable="false"
							tooltip="Project Coordinator" width="100%" class="inctmangUserinfoClass1" />
					</HBox>
					<HBox>
						<Label text="Name:" width="100%" class="inctureUserProfileLblClass1" />
						<HBox>
							<Input value="{ApprovalModel>/user/firstName}" editable="false"
								width="85%" class="inctmangUserinfoClass2" />
						</HBox>
					</HBox>
					<HBox>
						<Label text="Email:" width="100%" class="inctureUserProfileLblClass1" />
						<Input value="{ApprovalModel>/user/userEmail}" editable="false"
							width="96%" class="inctmangUserinfoClass3" />
					</HBox>
					<HBox>
						<Label text="Mobile:" width="100%" class="inctureUserProfileLblClass1" />
						<Input value="{ApprovalModel>/user/userMobile}" editable="false"
							width="85%" class="inctmangUserinfoClass4" />
					</HBox>
					<HBox>
						<Label text="Cost Ctr:" width="100%" class="inctureUserProfileLblClass1" />
						<Input value="{ApprovalModel>/user/userCostctr}" editable="false"
							width="105%" class="inctmangUserinfoClass5" />
					</HBox>
					<HBox>
						<Label text="Group:" width="100%" class="inctureUserProfileLblClass1" />
						<Input value="{ApprovalModel>/user/userGroup}" editable="false"
							width="83%" class="inctmangUserinfoClass6" />
					</HBox>
					<HBox>
						<Label text="Reported By :" width="100%"
							class="inctureUserProfileLblClass1" />
						<Input value="xyz" width="125%" editable="false"
							class="inctmangUserinfoClass7" />
					</HBox>
					<HBox>
						<Label text="Assigned To:" width="100%"
							class="inctureUserProfileLblClass1" />
						<Input value="{ApprovalModel>/incident/assignedTo}" width="97%"
							editable="false" class="DinctmangUserinfoClass8" />
					</HBox>
				</l:Grid>
			</Panel>
		</VBox>

		<VBox class="inctureCapexBox">
			<Panel expandable="false" expanded="true" headerText="INCIDENT INFORMATION "
				width="auto" class="tabClass">
				<l:Grid defaultSpan="L4 M12 S12">
					<HBox>
						<Label text="Incident Id:" class="inctureUserProfileLblClass1" />
						<Input value="{ApprovalModel>/incident/incidentId}"
							editable="false" width="85%" class="DinctmangCreatincdClass1" />
					</HBox>
					<HBox>
						<Label text="Line Of Business:" class="inctureUserProfileLblClass1"></Label>
						<Input value="{ApprovalModel>/incident/incidentLob}"
							editable="false" width="85%" class="DinctmangCreatincdClass1" />
					</HBox>
					<HBox>
						<Label text="Type Of:" class="inctureUserProfileLblClass1"></Label>
						<Input value="{ApprovalModel>/incident/incidentType}"
							editable="false" width="85%" class="DinctmangCreatincdClass1" />
					</HBox>
				</l:Grid>
				<l:Grid defaultSpan="L12 M12 S12">
					<VBox class="sapUiTinyMarginBottom">
						<Label text="Description:" width="100%"
							class="inctureUserProfileLblClass" />
						<TextArea editable="false"
							value="{ApprovalModel>/incident/incidentDescription}" height="100px"
							width="98%" />
						<layoutData>
							<FlexItemData growFactor="2" />
						</layoutData>
					</VBox>
				</l:Grid>
				<l:Grid defaultSpan="L6 M12 S12">
					<HBox>
						<Label text="Equipment:" width="100%" class="inctureUserProfileLblClass3" />
						<Input value="{ApprovalModel>/wo/workEquipment}" editable="false"
							width="85%" class="Dequi" />


					</HBox>
					<HBox>
						<Label text="Priority:" class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/incident/incidentPriority}"
							editable="false" width="80%" class="Dpriority" />
					</HBox>
					<HBox>
						<Label text="Action:" tooltip="" class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/incident/incidentAction}"
							editable="false" width="78%" class="DAction" />
					</HBox>
					<HBox>
						<Label text="Status:" tooltip="" class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/incident/incidentStatus}"
							editable="false" class="DStatus" width="73%" />

					</HBox>
					<HBox>
						<Label text="Work Center:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/wo/workCenter}" editable="false"
							width="95%" class="Dgroup" />

					</HBox>
					<HBox>
						<Label text="Assigned Group:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/incident/assignedGroup}"
							editable="false" width="95%" class="Dgroup" />
					</HBox>
					<HBox>
						<Label text="Assign Date On:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/incident/assignedDate}"
							editable="false" width="93%" class="assign_dt" />
					</HBox>
					<HBox>
						<Label text="Reported On:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/incident/reportedDate}"
							editable="false" width="85%" class="Dreport_on" />
					</HBox>
					<HBox>
						<Label text="Basic Start Date:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/incident/createdDate}"
							editable="false" width="93%" class="assign_dt" />
					</HBox>
					<HBox>
						<Label text=" Basic Finish Date:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/incident/finishDate}"
							editable="false" width="93%" class="Dbasic_finish_dt" />
					</HBox>
				</l:Grid>

			</Panel>
		</VBox>

		<VBox class="inctureCapexBox">
			<Panel expandable="false" expanded="true" headerText="WORK ORDER"
				width="auto" class="tabClass">
				<l:Grid defaultSpan="L4 M12 S12">
					<HBox>
						<Label text="Work Equipment:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/wo/workEquipment}" editable="false"
							width="90%" class="workEq" />
					</HBox>
					<HBox>
						<Label text="Work Center:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/wo/workCenter}" editable="false"
							width="90%" class="workCe" />
					</HBox>
					<HBox>
						<Label text="Func Locat:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/wo/funcLoc}" editable="false"
							width="85%" class="funcLo" />
					</HBox>
					<HBox>
						<Label text="Assembly:" tooltip=""
							class="inctureUserProfileLblClassaasmb1"></Label>
						<Input value="{ApprovalModel>/wo/assembly}" editable="false"
							width="85%" class="assembly" />
					</HBox>
					<HBox>
						<Label text="Control Key:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/wo/controlKey}" editable="false"
							width="85%" class="controlKey" />
					</HBox>
					<HBox>
						<Label text="Activity Type:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/wo/activityType}" editable="false"
							width="85%" class="activityTy" />
					</HBox>
					<HBox>
						<Label text="Planning Group:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/wo/planningGroup}" editable="false"
							width="85%" class="planningGrp" />
					</HBox>

					<HBox>
						<Label text="Work Type:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/wo/workType}" editable="false"
							width="85%" class="workTy" />
					</HBox>
					<HBox>
						<Label text="Planning Plant:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/wo/planningPlant}" editable="false"
							width="85%" class="planning_plnt" />
					</HBox>
					<HBox>
						<Label text="Business Area:" tooltip=""
							class="inctureUserProfileLblClass3"></Label>
						<Input value="{ApprovalModel>/wo/busArea}" editable="false"
							width="85%" class="businessAr" />
					</HBox>
				</l:Grid>
			</Panel>
		</VBox>
		<Button text="Close" class="Dinctmang_create" press="onclose" />

	</Dialog>
</core:FragmentDefinition>