							liveChange="handleLiveChange" valueState="Warning" valueLiveUpdate="true"/>
						<!--<layoutData>
											<FlexItemData growFactor="2"/>
										</layoutData>-->
					</VBox>
				</l:Grid>
				<l:Grid defaultSpan="L6 M12 S12">
