						<Label text="Assigned Group:" tooltip="" class="inctureUserProfileLblClass3"></Label>
						<ComboBox selectedKey="{mod4>/Assignd_grp}" class="inctmang_group" width="100%" showSecondaryValues="true">
							
							<core:ListItem key="Inprogress" text="In progress"/>
							<core:ListItem key="Completed" text="Completed"/>
							<core:ListItem key="Reopen" text="Reopen"/>
						</ComboBox>
					</HBox>
					<HBox>
						<Label text="Assign Date On:" tooltip="" class="inctureUserProfileLblClass3"></Label>
						<DateTimePicker value="{mod4>/Assig_dt}" id="assigndate" class="inctmang_assdt" width="106%" placeholder="Enter Date Time..." displayFormat="short"/>
					</HBox>
					<HBox>
						<Label text="Reported On:" tooltip="" class="inctureUserProfileLblClass3"></Label>
						<DatePicker value="{mod4>/Reprt_on}" id="reportedon" class="inctmang_report_on" width="95%" placeholder="Enter Date... " displayFormat="short"/>
					</HBox>
					<HBox>
						<Label text="Basic Start Date:" required="true" tooltip="" class="inctureUserProfileLblClass3" ></Label>
						<DateTimePicker  id="date_start" class="inctmang_startdt" width="110%" value="{model>/startdate}" change="startdate" placeholder="Enter Date Time..." displayFormat="short"/>
					</HBox>
					<HBox>
						<Label text=" Basic Finish Date:" tooltip="" class="inctureUserProfileLblClass3"></Label>
						<DateTimePicker value="{mod4>/finishdt}" id="date_finish" class="inctmang_type1" width="108%" placeholder="Enter Date Time..." displayFormat="short"/>
					</HBox>
					
