						<ComboBox selectedKey="{mod4>/lob}" class="inctmang_lob1" width="90%" items="{model>/LOB}" selectionChange="onLOBselection">
							<items>
								<core:Item key="{model>title}" text="{model>title}" />
							</items>
						</ComboBox>
					</HBox>
					<HBox>
						<Label text="Type Of:" class="inctmang_type2"></Label>
						<ComboBox selectedKey="{mod4>/type}" class="inctmang_type1"  width="80%" items="{oModel>/}">
